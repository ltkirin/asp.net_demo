@model List<AspDemo.DomainModel.common.model.ListItemModel>
@{
    ViewData["Title"] = "Учредители";
}

<h2>Учредители</h2>
<div>
    <form asp-action="Add">
        <input type="submit" value="Добавить учредителя" class="btn btn-primary" />
    </form>
</div>
<div>
    <lu>
        @foreach (AspDemo.DomainModel.common.model.ListItemModel item in Model)
        {
            <li>
                <a href="/Founders/Open/@item.Id">@item.Value</a>
            </li>
        }
    </lu>
</div>

